{
  "name": "graphsify",
  "homepage": "https://abernardi597.github.io/graphsify",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "material-ui": "^1.0.0-beta.40",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "xo && react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "xo": "^0.20.3"
  },
  "xo": {
    "space": true,
    "rules": {
      "linebreak-style": 0,
      "no-else-return": [
        "off"
      ],
      "curly": [
        "off"
      ]
    },
    "envs": [
      "browser"
    ],
    "plugins": [
      "react"
    ],
    "extends": [
      "plugin:react/recommended"
    ]
  }
}
